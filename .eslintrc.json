{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true,
      "globalReturn": false
    },
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 1,
    "react/jsx-uses-vars": [2],
    "no-console": 0
  }
}
